<UserControl x:Class="PrismTest.Views.SecondView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrismTest.Views"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Background="Cyan">
    <UserControl.LayoutTransform>
        <ScaleTransform />
    </UserControl.LayoutTransform>
    <StackPanel Width="300">
        <TextBlock Text="SecondView" HorizontalAlignment="Center" />
        <Button Command="{Binding GoBackCommand}" Content="GoBack" Margin="20" />
    </StackPanel>
</UserControl>
